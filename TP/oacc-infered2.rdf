    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#Cryptocurrency -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#Cryptocurrency">
        <desc:about xml:lang="en">A cryptocurrency.</desc:about>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#DistributionScheme -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#DistributionScheme">
        <desc:about xml:lang="en">How the initial distribution of coins should be conducted between miners and non-miners.</desc:about>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#Fork -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#Fork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#DistributionScheme"/>
        <desc:about xml:lang="en">When a coin forks from another coin, all existing balances on the previous coin wallet are replicated on the new coin wallet on a ration set by the developer.</desc:about>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#HashingAlgorithm -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#HashingAlgorithm">
        <desc:about xml:lang="en">Cryptographic function for producing hashes/results.</desc:about>
        <desc:title xml:lang="en">Cryptographic hashing function</desc:title>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#ICO -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#ICO">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#DistributionScheme"/>
        <desc:about xml:lang="en">An ICO (Initial Coin Offering) is used by startups to bypass the rigorous and regulated capital-raising process required by venture capitalists or banks.
The main purpose is to get sufficient hard-currency in exchange for an ammount of Cryptocurrency.</desc:about>
        <desc:title xml:lang="en">Initial Coin Offering</desc:title>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#None -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#None">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#DistributionScheme"/>
        <desc:about xml:lang="en">Used when no premine or no ICO was offered.
Fair launch for all peers.</desc:about>
        <desc:title xml:lang="en">Fair launch, no ICO or pre-mine</desc:title>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#POS -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#POS">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#ProtectionScheme"/>
        <desc:about xml:lang="en">A method of securing a cryptocurrency peer-to-peer network through requesting users to show ownership of a certain amount of currency.</desc:about>
        <desc:title xml:lang="en">Proof of Stake</desc:title>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#POW -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#POW">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#ProtectionScheme"/>
        <desc:about xml:lang="en">A method of securing a cryptocurrency peer-to-peer network through requesting proof from users that work has been done.</desc:about>
        <desc:title xml:lang="en">Proof of Work</desc:title>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#Premine -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#Premine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#DistributionScheme"/>
        <desc:about xml:lang="en">A premine is when a developer allocates a certain amount of currency credit to a particular address before releasing the source code to the open community. This is often done based on the reasoning that they need to get hard-currency for exchange listing, etc.</desc:about>
        <desc:title xml:lang="en">Pre-mine</desc:title>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#ProtectionScheme -->

    <owl:Class rdf:about="http://www.semanticweb.org/octavio/ontologies/2018/5/oacc#ProtectionScheme">
        <desc:about xml:lang="en">How the public ledger/record should be mantained.</desc:about>
    </owl:Class>
    


